{
  "package.json": {
    "prefix": "pk",
    "body": [
      "{",
        "  \"name\": \"@${TM_DIRECTORY/.+\\/(.+\\/.+)$/$1/i}\",",
        "  \"version\": \"1.0.0\",",
        "  \"private\": true,",
        "  \"description\": \"$1\",",
        "  \"main\": \"src/index.ts\",",
        "  \"type\": \"module\",",
        "  \"scripts\": {",
        "    \"dev\": \"vite --port 1${RANDOM/^(\\d\\d\\d\\d).+/$1/} --open\",",
        "    \"build\": \"vite build --emptyOutDir\",",
        "    \"eslint\": \"eslint . --fix --ext .js,.jsx,.cjs,.mjs,.ts,.tsx\",",
        "    \"test\": \"vitest\"",
        "  },",
        "  \"keywords\": [",
        "    \"${TM_DIRECTORY/.+\\/(.+)\\/.+$/$1/}\",",
        "    \"${TM_DIRECTORY/.+\\/(.+)$/$1/}\",",
        "    \"${TM_DIRECTORY/.+\\/(.+\\/.+)$/$1/}\",",
        "    \"usePropStyles\",",
        "    \"use-prop-styles\",",
        "    \"vue-hook\",",
        "    \"react-hook\",",
        "    \"front-end\"",
        "  ],",
        "  \"files\": [],",
        "  \"workspaces\": [],",
        "  \"lint-staged\": {",
        "    \"*.{js,ts,cjs,mjs,ts,tsx}\": \"eslint --fix\"",
